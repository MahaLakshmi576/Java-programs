import java.util.Scanner;
class sum{
    public static void main(String []args){
        Scanner in =new Scanner(System.in);
        System.out.println("Enter val a:");
    int a=in.nextInt();
    System.out.println("Enter val b:");
    int b=in.nextInt();
    
    int sum= a+b;
    System.out.println("Sum of two numbers:"+sum);  
   
    }
   
}

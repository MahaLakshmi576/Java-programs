import java.util.Scanner;

public class hello {
    public static  void main(String[] args){
        Scanner in =new Scanner(System.in);
     System.out.println("Enter value:");
     int i=in.nextInt();


         if (i%2 == 0){
             System.out.println(i +" " + "is Even Number:");

         }
     else{
             System.out.println(i +" "+ "is Odd Number:" );

     }
    }
}

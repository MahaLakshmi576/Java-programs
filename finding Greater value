import java.util.Scanner;
class greter_val{
    public static void main(String []args){
    Scanner in=new Scanner(System.in); 
    System.out.println("Enter val a:");
    int a=in.nextInt();
    System.out.println("Enter val b:");
    int b=in.nextInt();
    System.out.println("Enter val c:");
    int c=in.nextInt();
    if(a>b && a>c){
        System.out.println("a  is greater value");
    }
    else if(b>c){
        System.out.println("b is greater value");
    }
    else{
    System.out.println("c is greater value");
        
    }
    
}
   }
